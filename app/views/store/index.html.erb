<h3>Categories</h3>
<% @categories.each do |category| %>
  <%= link_to category.title, category_path(category) %><br>
<% end %>

<h3>Items</h3>
<% @items.each do |item| %>
  <p><%= item.title %> | <%= number_to_currency(item.price/100) %> | Inventory: <%= item.inventory %>  <%= (button_to 'Add to Cart', line_items_path(item_id: item)) if user_signed_in? %></p>
<% end %>

<%= current_user.email if current_user %><br>
